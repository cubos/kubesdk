{
  "name": "kubeoperator",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/main.ts",
    "test": "$(node spec/setup.js) && jest -w10",
    "eslint:fix": "eslint --fix '{src,spec}/**/*.ts'",
    "eslint:check": "eslint '{src,spec}/**/*.ts'",
    "prettier:fix": "prettier --write '{src,spec}/**/*.{t,j}s'",
    "prettier:check": "prettier --check '{src,spec}/**/*.{t,j}s'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ajv": "^6.12.5",
    "axios": "^0.20.0",
    "axios-logger": "^2.4.0",
    "js-yaml": "^3.14.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.14",
    "@types/js-yaml": "^3.12.5",
    "@types/node": "^14.14.5",
    "@types/ws": "^7.2.7",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "@typescript-eslint/typescript-estree": "^4.0.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jest-extended": "^0.11.5",
    "jest": "^26.5.3",
    "prettier": "^2.0.4",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "jest": {
    "preset": "ts-jest",
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/spec/**/*.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/spec/utils.ts"
    ],
    "verbose": true,
    "testTimeout": 60000,
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "setupFilesAfterEnv": [
      "jest-extended"
    ]
  }
}
